

/* 切换 */
.swiper-tab{
    width:750rpx;
    height:124rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background:white;
    border-bottom: 2rpx solid #ccc;
    /* box-shadow: 0 2rpx 12rpx #ccc; */
}
.right{
    border-radius: 10rpx 0rpx 0rpx 10rpx;
    
}
.left{
    border-radius: 0rpx 10rpx 10rpx 0rpx;
}

.swiper-tab-list{  
    font-size: 30rpx;  
    display: inline-block;  
    width: 25.5%;  
    height: 58rpx; 
    text-align: center;
    line-height: 58rpx;
    border: 1px solid #00a2e9;
    color: #777777;  
    
}  
.on{ 
    color: #badffc; 
    background: #00a2e9;
    border-bottom: 5rpx solid #00a2e9;

}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
