/* 切换 */
.swiper-tab{
    width:676rpx;
    height:75rpx;
    /* margin: 20rpx; */
    padding-left: 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    text-align: center;
    background:white;
}
.swiper-tab-list{ 
  
    font-size: 30rpx;  
    display: inline-block;  
    width: 16.3%;  
    height:66rpx;
    color: #777777;  
} 
